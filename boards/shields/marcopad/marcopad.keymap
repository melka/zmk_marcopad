#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
    &lt 1 A    &kp B    &kp C    &kp D
    &kp E      &kp F    &kp G    &kp H
    &kp I      &kp J    &kp K    &kp L
    &kp M      &kp N    &kp O    &kp P
            >;
        };
        function_layer {
            bindings = <
    &kp A      &kp B      &kp C      &kp D
    &kp E      &kp F      &kp G      &kp H
    &kp I      &kp J      &kp K      &kp L
    &bt BT_CLR &bt BT_PRV &bt BT_NXT &bootloader
            >;
        };
    };
};
